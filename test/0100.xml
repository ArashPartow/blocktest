<?xml version="1.0"?>
<testbody>
	<info note="ICub right ankle roll pwm injection." shortnote="" version="1" />
	<logging loggingtype="position" loggingpart="r_ankle_roll r_ankle_pitch" loggingwrappername="/right_leg" />
	
	<command library="yarp" name="yarpreset" repetitions="1" wait="0" reporterror="true" />
	<command library="yarp" name="yarpwait" seconds="1" repetitions="1" wait="0" reporterror="true" />
	<command library="yarp" name="yarpsendpos" wrappername="/right_leg" jointname="r_ankle_roll" degree="0" velocity="20" repetitions="1" wait="0" reporterror="true" />
	<command library="yarp" name="yarpwait" seconds="1" repetitions="1" wait="0" reporterror="true" />
	<command library="yarp" name="yarpsendpwm" wrappername="/right_leg" jointname="r_ankle_roll" profile="const" frequency="1" dutycycle="20" time="2" repetitions="1" wait="0" reporterror="true" />
	<command library="yarp" name="yarpwait" seconds="1" repetitions="1" wait="0" reporterror="true" />
	<command library="yarp" name="yarpcheckjointposition" wrappername="/right_leg" jointname="r_ankle_roll" tolerance="1" expectedvalue="7" repetitions="1" wait="0" reporterror="true" />
	<command library="yarp" name="yarpwait" seconds="1" repetitions="1" wait="0" reporterror="true" />
	<command library="yarp" name="yarpsendpos" wrappername="/right_leg" jointname="r_ankle_roll" degree="0" velocity="20" repetitions="1" wait="0" reporterror="true" />
	<command library="yarp" name="yarpwait" seconds="2" repetitions="1" wait="0" reporterror="true" />
	<command library="general" name="execute" command="python3" param=".//test//files//plot.py 1 log//pos_0100-r_ankle_roll-0 Ankle_roll_0100" prefix="" tag="exe1" waitafter="2" kill="no" repetitions="1" wait="0" reporterror="true" />
	<command library="general" name="execute" command="python3" param=".//test//files//plot.py 1 log//pos_0100-r_ankle_pitch-0 Ankle_pitch_0100" prefix="" tag="exe2" waitafter="2" kill="no" repetitions="1" wait="0" reporterror="true" />
</testbody>
